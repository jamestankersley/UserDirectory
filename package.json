{
  "name": "userdirectory",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamestankersley/UserDirectory.git"
  },
  "author": "James Tankersley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamestankersley/UserDirectory/issues"
  },
  "homepage": "https://github.com/jamestankersley/UserDirectory#readme",
  "dependencies": {
    "concurrently": "^5.1.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4"
  }
}
